<div class="post-wrapper row">
  <div id="posts">
    <% @posts.each do |post| %>
      <%= render 'post', post: post %>

    <% end %>
  <%= paginate @posts %>
</div>
  <div id="paginator" class="text-center">
    <%= link_to_next_page @posts, "LOAD MORE", remote: true, id:'load_more' %>
  </div>
</div>
